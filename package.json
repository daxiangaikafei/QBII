{
    "name": "qbao_goods",
    "version": "1.0.1",
    "description": "Get started with React, Redux, and React-Router!",
    "main": "index.js",
    "engines": {
        "node": ">=4.2.0",
        "npm": "^3.0.0"
    },
    "scripts": {
        "dev": "better-npm-run dev",
        "dev:nw": "npm run dev -- --nw",
        "compile": "npm run clear && better-npm-run deploy",
        "product": "npm run clear && better-npm-run deploys",
        "clear": "rm -rf dist"
    },
    "betterScripts": {
        "dev": {
            "command": "nodemon -r dotenv/config bin/server",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "deploy": {
            "command": "node -r dotenv/config bin/compile",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "deploys": {
            "command": "node -r dotenv/config bin/compile",
            "env": {
                "NODE_ENV": "build"
            }
        }
    },
    "author": "sean",
    "license": "MIT",
    "dependencies": {
        "babel-register": "^6.18.0",
        "better-npm-run": "0.0.13",
        "bluebird": "^3.4.7",
        "debug": "^2.4.4",
        "dotenv": "^2.0.0",
        "dva": "^1.2.1",
        "es6-promise": "^4.0.5",
        "fastclick": "^1.0.6",
        "file-loader": "^0.9.0",
        "koa": "^2.0.0-alpha.3",
        "koa-connect-history-api-fallback": "^0.3.0",
        "koa-convert": "^1.2.0",
        "koa-static": "^2.0.0",
        "lodash": "^4.17.3",
        "node-sass": "^4.5.0",
        "nodemon": "^1.11.0",
        "react": "^15.4.1",
        "react-addons-linked-state-mixin": "^15.6.0",
        "react-mixin": "^3.0.5",
        "react-redux": "^5.0.1",
        "react-router": "^3.0.0",
        "redux": "^3.6.0",
        "redux-actions": "^1.2.0",
        "redux-simple-router": "^2.0.4",
        "sass-loader": "^6.0.3",
        "velocity-react": "^1.1.11",
        "whatwg-fetch": "^2.0.1",
        "yargs": "^6.5.0"
    },
    "devDependencies": {
        "autoprefixer": "^6.6.1",
        "babel-core": "^6.21.0",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.2.10",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-transform-object-assign": "^6.8.0",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "^6.18.0",
        "babel-preset-react": "^6.16.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.16.0",
        "babel-runtime": "^6.20.0",
        "classnames": "^2.2.5",
        "css-loader": "^0.26.1",
        "cssnano": "^3.9.1",
        "eslint": "^3.12.2",
        "eslint-config-standard": "^6.2.1",
        "eslint-config-standard-react": "^4.2.0",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-react": "^6.8.0",
        "eslint-plugin-standard": "^2.0.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "fs-extra": "^1.0.0",
        "history": "^2.0.0-rc2",
